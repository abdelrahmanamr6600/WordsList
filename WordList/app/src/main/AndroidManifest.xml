<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abdelrahmanamr660.wordlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WordList">
        <activity android:name="com.example.abdelrahmanamr660.wordlist.ui.AddWordActivity"
            android:parentActivityName="com.example.abdelrahmanamr660.wordlist.ui.MainActivity"></activity>
        <activity android:name="com.example.abdelrahmanamr660.wordlist.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>